{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node --require ./jest-shims/register.js ./node_modules/jest/bin/jest.js",
    "test:watch": "node --require ./jest-shims/register.js ./node_modules/jest/bin/jest.js --watch",
    "test:cov": "node --require ./jest-shims/register.js ./node_modules/jest/bin/jest.js --coverage",
    "test:debug": "node --inspect-brk -r ./jest-shims/register.js -r tsconfig-paths/register -r ts-node/register ./node_modules/jest/bin/jest.js --runInBand",
    "test:e2e": "node --require ./jest-shims/register.js ./node_modules/jest/bin/jest.js --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.16.3",
    "bullmq": "^5.61.0",
    "cache-manager": "^7.2.4",
    "cache-manager-redis-yet": "^5.1.5",
    "cheerio": "^1.0.0",
    "class-validator": "^0.14.2",
    "cron": "^2.4.4",
    "got": "^14.6.1",
    "ioredis": "^5.4.1",
    "puppeteer": "^23.11.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tough-cookie": "^6.0.0",
    "vk-io": "^4.10.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.16.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "<rootDir>/../jest-shims/ts-transform.js"
    },
    "moduleNameMapper": {
      "^vk-io$": "<rootDir>/../jest-shims/node_modules/vk-io/index.js",
      "^@prisma/client$": "<rootDir>/../jest-shims/node_modules/@prisma/client/index.js",
      "^@nestjs/cache-manager$": "<rootDir>/../jest-shims/node_modules/@nestjs/cache-manager/index.js",
      "^@nestjs/bullmq$": "<rootDir>/../jest-shims/node_modules/@nestjs/bullmq/index.js",
      "^bullmq$": "<rootDir>/../jest-shims/node_modules/bullmq/index.js",
      "^libphonenumber-js/max$": "<rootDir>/../jest-shims/node_modules/libphonenumber-js/max.js",
      "^got$": "<rootDir>/../jest-shims/node_modules/got/index.js",
      "^cheerio$": "<rootDir>/../jest-shims/node_modules/cheerio"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.module\\.ts$",
      "main.ts"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
